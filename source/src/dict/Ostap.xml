<?xml version="1.0" encoding="ISO-8859-1"?>

<lcgdict>
  
  <class pattern    = "Ostap::*"                    />
  <class pattern    = "Ostap::Math::*"              />  
  <class pattern    = "Ostap::Math::GSL::*"         />  
  <class pattern    = "Ostap::Utils::*"             />
  <class pattern    = "Ostap::Models::*"            />
  <class pattern    = "Ostap::Functions::*"         />
  <class pattern    = "Ostap::Kinematics::*"        />
  <class pattern    = "Ostap::MoreRooFit::*"        />
  <class pattern    = "Ostap::Math::FormFactors::*" />
  
  <function pattern = "Ostap::Math::*"            />
  <function pattern = "Ostap::Utils::*"           />
  <function pattern = "Ostap::Utils::Histos::*"   />
  <function pattern = "Ostap::Kinematics::*"      />
  
  <function name    = "Ostap::TMVA::addResponse"  />
  
  <class name    = "std::function&lt;double(double,double,double,double)&gt;" />
  <class pattern = "std::function&lt;double(*)&gt;" />
  
  <class name    = "Ostap::Math::Tensors::Epsilon"  />
  
  <class pattern = "std::vector&lt;Ostap::WStatEntity,*&gt;" />
  
  <class name   = "Ostap::Math::WorkSpace">
    <field name = "m_workspace"         transient="true"/>      
    <field name = "m_workspace_cquad"   transient="true"/>      
    <field name = "m_workspace_romberf" transient="true"/>      
  </class>
  
  <class name   = "Ostap::Exception">
    <field name = "m_previous"          transient="true"/>      
  </class>
  
  <class name = "Ostap::IFuncTree"  />
  <class name = "Ostap::IFuncData"  />
  
  <exclusion>
     
    <class name    = "Ostap::StatVar::Interval"     />
    <class name    = "Ostap::Math::Interpolation::DATAVCT" />    
    <class name    = "Ostap::Math::Bernstein2D::VB" />
    <class name    = "Ostap::Math::Integrator"      />  

    <class pattern = "Ostap::Math::details::*"      />
    <class pattern = "Ostap::Math::Models::*"       />
    <class pattern = "Ostap::Utils::details::*"     />
    <class pattern = "Ostap::Math::TypeWrapper*"    />
    <class pattern = "ROOT::Math::SVector*" />
    <class pattern = "ROOT::Math::Plane3D*" />
    <class pattern = "ROOT::Math::SMatrix*" />
    <class pattern = "ROOT::Math::PositionVector*"     />
    <class pattern = "ROOT::Math::DisplacementVector*" />
    <class pattern = "ROOT::Math::LorentzVector*"      />
    <class pattern = "ROOT::Math::Impl::Plane3D*"      />

    <class    pattern = "gsl_*"    />
    <function pattern = "gsl_*"    />
    <class    pattern = "TMatrix*" />
    <class    pattern = "TVector*" />
    
    <class name    = "Ostap::Utils::Tee">
      <field name  = "m_file"   />      
      <field name  = "m_buffer" />      
      <field name  = "m_keep"   />      
    </class>

    <class name    = "Ostap::Math::WorkSpace">
      <field name  = "m_workspace"/>      
    </class>
    
    <class name    = "Ostap::Exception">
      <field name  = "m_previous"/>      
    </class>
    
    <class name    = "Ostap::Math::Bernstein2D">
      <field name  = "m_bx"/>      
      <field name  = "m_by"/>      
    </class>

    <class name    = "Ostap::Math::Bernstein3D">
      <field name  = "m_bx"/>      
      <field name  = "m_by"/>      
      <field name  = "m_bz"/>      
    </class>

    <class name    = "Ostap::Math::Bernstein2DSym">
      <field name  = "m_b"/>      
    </class>
    
    <class name    = "Ostap::Math::Chi2Fit">  
      <field name  = "m_solu" />      
      <field name  = "m_cov2" />      
    </class>
    
    <class name    = "Ostap::Functions::FuncFormula">  
      <field name  = "m_formula" />      
    </class>

    <class name    = "Ostap::Functions::FuncRooFormula">  
      <field name  = "m_formula" />      
    </class>

    <class function = "Ostap::Math::poly_to_bernstein"     />  
    <class function = "Ostap::Math::bernstein_from_roots"  />  

  </exclusion>  
</lcgdict>
